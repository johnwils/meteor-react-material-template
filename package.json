{
  "name": "meteor-react-reactrouter-material-template",
  "private": true,
  "scripts": {
    "start": "meteor run --settings settings-local-dev.json",
    "start:e2e": "meteor run --port 3050 --settings settings-local-dev.json",
    "lint": "eslint \"imports/**/*.js\" \"cypress/**/*.js\"; exit 0",
    "cypress:open": "cypress open",
    "cypress:headless": "cypress run --config video=false",
    "test:e2e": "start-server-and-test start:e2e http://localhost:3050 cypress:open",
    "test:e2e:ci": "start-server-and-test start:e2e http://localhost:3050 cypress:headless",
    "unit-test": "NODE_ENV=UNIT_TESTING jest",
    "prettier": "prettier --write \"client/**/*.js\" \"imports/**/*.js\" \"server/**/*.js\" \"cypress/**/*.js\" --single-quote true --trailing-comma es5"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/enzyme-config.js",
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@material/linear-progress": "^0.43.0",
    "@material/typography": "^0.44.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.4",
    "lodash": "^4.17.11",
    "meteor-node-stubs": "^0.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "remove": "^0.1.5",
    "simpl-schema": "^1.5.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.3.1",
    "@meteorjs/eslint-config-meteor": "^1.0.5",
    "autoprefixer": "^9.4.7",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chromedriver": "^2.46.0",
    "cypress": "^3.1.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-meteor": "^0.1.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-meteor": "^5.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "regenerator-runtime": "^0.13.1",
    "start-server-and-test": "^1.7.11"
  }
}

{
  "name": "meteor-react-reactrouter-material-template",
  "private": true,
  "scripts": {
    "start": "meteor run --settings settings-local-dev.json",
    "start:e2e": "meteor run --port 3050 --settings settings-local-dev.json",
    "lint": "eslint \"imports/**/*.js\"; exit 0",
    "cypress:open": "cypress open",
    "cypress:headless": "cypress run --config video=false",
    "test:e2e": "start-server-and-test start:e2e http://localhost:3050 cypress:open",
    "test:e2e:ci": "start-server-and-test start:e2e http://localhost:3050 cypress:headless",
    "test:unit": "NODE_ENV=UNIT_TESTING jest",
    "test:server": "mv ./cypress ./private/cypress && TEST_CLIENT=0 meteor test --once --driver-package meteortesting:mocha && mv ./private/cypress ./cypress",
    "prettier": "prettier --write \"client/**/*.js\" \"imports/**/*.js\" \"server/**/*.js\" \"cypress/**/*.js\" --single-quote true --trailing-comma es5",
    "clean": "rm -rf node_modules/ package-lock.json && meteor npm i"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./tests/enzyme-config.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "./cypress/"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.5.4",
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.2.0",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "lodash": "^4.17.14",
    "meteor-node-stubs": "^0.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "recompose": "^0.30.0",
    "simpl-schema": "^1.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@meteorjs/eslint-config-meteor": "^1.0.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "chai": "^4.2.0",
    "chromedriver": "^75.1.0",
    "cypress": "^3.4.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-meteor": "^5.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.2",
    "start-server-and-test": "^1.9.1"
  }
}
